{
  "name": "chime-sdk-telehealth-widget-frontend-demo",
  "version": "1.0.0",
  "private": true,
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.4.1",
    "@aws-amplify/core": "^4.3.14",
    "@aws-amplify/storage": "^4.4.15",
    "@aws-amplify/ui-react": "^2.20.0",
    "@aws-sdk/client-chime-sdk-identity": "^3.145.0",
    "@aws-sdk/client-chime-sdk-messaging": "^3.145.0",
    "@aws-sdk/client-cognito-identity": "^3.52.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.145.0",
    "@aws-sdk/client-lambda": "^3.145.0",
    "@aws-sdk/client-sfn": "^3.145.0",
    "@aws-sdk/credential-providers": "^3.145.0",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typed-document-node": "^2.3.6",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-query": "^4.0.5",
    "@tanstack/react-query": "^4.0.10",
    "@types/node": "^16.11.47",
    "@types/react": "^17.0.48",
    "@types/react-datetime-picker": "^3.4.1",
    "@types/react-dom": "^17.0.17",
    "amazon-chime-sdk-component-library-react": "^3.2.0",
    "amazon-chime-sdk-js": "^3.6.0",
    "amplify-graphql-docs-generator": "^2.2.4",
    "aws-amplify": "^4.3.30",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "i18next": "^21.9.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "install": "^0.13.0",
    "npm": "^8.16.0",
    "react": "^17.0.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.5",
    "react-i18next": "^11.18.3",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "zen-observable-ts": "^1.1.0"
  },
  "scripts": {
    "build:widget": "rm -rf widget-dist && parcel build src/index.tsx --no-source-maps --log-level warn --dist-dir widget-dist",
    "build": "react-scripts build",
    "deploy": "npm run build:widget && npx ts-node --project scripts/tsconfig.json scripts/deploy.ts",
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "copy-schema": "cp ./../backend/appsync/schema.graphql ./schema.graphql",
    "generate-statements": "node bin/generateStatements.js",
    "codegen": "cd ../backend && yarn synth && cd ../frontend && yarn run copy-schema && yarn run generate-statements && graphql-codegen --config codegen.yml && rm schema.graphql"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.145.0",
    "@aws-sdk/client-s3": "^3.145.0",
    "@svgr/webpack": "^6.3.1",
    "@types/react-datepicker": "^4.4.2",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.13",
    "chalk": "^4.1.2",
    "parcel": "^2.7.0",
    "postcss": "^8.4.19",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  }
}
